{
  "name": "app",
  "private": true,
  "scripts": {
    "lint": "eslint ./lib ./client ./server --ext .js",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001 -s ./public",
    "start": "meteor run"
  },
  "dependencies": {
    "cls-bluebird": "^1.0.1",
    "continuation-local-storage": "^3.1.6",
    "mantra-core": "^1.4.0",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.12.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-mounter": "^1.1.0",
    "request-promise": "^2.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.4.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-react-require": "^2.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-meteor": "^6.6.7",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-root-slash-import": "^1.0.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.3.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "sinon": "^1.17.3"
  }
